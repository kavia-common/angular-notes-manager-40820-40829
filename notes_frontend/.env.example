# Notes Frontend environment configuration
# Copy this file to .env and set values for your environment.

NG_APP_API_BASE=/api
NG_APP_BACKEND_URL=http://localhost:3000
NG_APP_FRONTEND_URL=http://localhost:3000
NG_APP_WS_URL=ws://localhost:3000
NG_APP_NODE_ENV=development
NG_APP_NEXT_TELEMETRY_DISABLED=1
NG_APP_ENABLE_SOURCE_MAPS=false
NG_APP_PORT=3000
NG_APP_TRUST_PROXY=false
NG_APP_LOG_LEVEL=info
NG_APP_HEALTHCHECK_PATH=/healthz
NG_APP_FEATURE_FLAGS=
NG_APP_EXPERIMENTS_ENABLED=false
